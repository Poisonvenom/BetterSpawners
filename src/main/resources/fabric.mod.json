{
  "schemaVersion": 1,
  "id": "spawner-mask",
  "version": "1.0.0",
  "name": "SpawnerMask",
  "description": "Mod to hide a spawner and make it appear as if no one has visited it.",
  "authors": [
    "poisonvenom"
  ],
  "contact": {
    "repo": "https://github.com/poisonvenom"
  },
  "icon": "assets/icon/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "net.poisonvenom.spawnermask.SpawnerMask"
    ]
  },
  "accessWidener": "spawnermask.accesswidener",
  "mixins": [
    "spawnermask.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "255,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}