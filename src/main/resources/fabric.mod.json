{
  "schemaVersion": 1,
  "id": "better-spawners",
  "version": "1.1.0",
  "name": "Better Spawners",
  "description": "Mod to hide a spawner and make it appear as if no one has visited it.",
  "authors": [
    "poisonvenom"
  ],
  "contact": {
    "repo": "https://github.com/poisonvenom"
  },
  "icon": "assets/icon/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "net.poisonvenom.betterspawners.BetterSpawners"
    ]
  },
  "accessWidener": "betterspawners.accesswidener",
  "mixins": [
    "betterspawners.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "255,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}